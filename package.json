{
  "name": "nextjs-microcms-blog",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "dev:api": "aspida --watch",
    "build": "pathpida && aspida && next build",
    "start": "next start",
    "test": "jest",
    "fix": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "lint-staged": "lint-staged",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "next lint . --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "highlight.js": "^11.0.0",
    "microcms-richedit-processer": "^1.0.1",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scroll": "^1.8.2",
    "react-use": "^17.2.4",
    "rss": "^1.2.2",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@next/bundle-analyzer": "^11.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.14",
    "@types/react-scroll": "^1.8.3",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^11.0.4",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.15.2",
    "postcss": "^8.3.5",
    "postcss-custom-properties": "^11.0.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.3.5"
  }
}
